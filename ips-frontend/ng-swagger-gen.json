{
  "$schema": ".//node_modules/ng-swagger-gen/ng-swagger-gen-schema.json",
  "swagger": "../pw-05-backend/src/main/swagger/api-specification.yaml",
  "output": "src/app/api",
  "prefix": "Api",
  "ignoreUnusedModels": true,
  "minParamsForContainer": 2,
  "sortParams": "desc",
  "defaultTag": "Api",
  "removeStaleFiles": true,
  "modelIndex": true,
  "serviceIndex": true,
  "apiModule": true,
  "enumModule": true,
  "generateExamples": false
}
